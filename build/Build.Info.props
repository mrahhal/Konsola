<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<!-- Versioning -->
		<MajorVersion>0</MajorVersion>
		<MinorVersion>3</MinorVersion>
		<PatchVersion>1</PatchVersion>
		<PrereleaseTag></PrereleaseTag>
		
		<PrereleaseTagWithDash Condition="'$(PrereleaseTag)' != ''">-$(PrereleaseTag)</PrereleaseTagWithDash>

		<VersionWithoutTag>$(MajorVersion).$(MinorVersion).$(PatchVersion)</VersionWithoutTag>
		<Version>$(VersionWithoutTag)$(PrereleaseTagWithDash)</Version>
		<!--============-->
		
		<!-- Assembly info -->
		<AssemblyDescription>Contains services for command line tools.</AssemblyDescription>
		<!--============-->
	</PropertyGroup>
</Project>