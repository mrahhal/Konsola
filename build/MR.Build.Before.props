<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<Configuration Condition="'$(Configuration)' == ''">Release</Configuration>
		<BuildDir>$(MSBuildThisFileDirectory)</BuildDir>
		<SolutionDir>$(BuildDir)..\</SolutionDir>
		<ArtifactsDir>$(SolutionDir)artifacts\</ArtifactsDir>
		<BinariesDir>$(ArtifactsDir)binaries\</BinariesDir>
		<PackagesDestinationDir>$(ArtifactsDir)packages</PackagesDestinationDir>
		<NuspecsSrc>$(SolutionDir)nuspecs\</NuspecsSrc>
		<NuspecsDestination>$(ArtifactsDir)nuspecs\</NuspecsDestination>
	</PropertyGroup>
</Project>